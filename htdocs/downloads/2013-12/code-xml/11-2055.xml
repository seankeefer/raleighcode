<law><law_id>396</law_id><section_number>11-2055</section_number><catch_line>UNAUTHORIZED TRAFFIC-CONTROL DEVICES.</catch_line><history/><order_by>11-2055</order_by><metadata/><url>/11-2055/</url><token>11-2055</token><structure><unit label="division" level="1" identifier="II">Division II - Planning and Development</unit><unit label="part" level="3" identifier="11">PART 11 - TRANSPORTATION</unit><unit label="chapter" level="4" identifier="2">CHAPTER 2. - MOTOR VEHICLES AND TRAFFIC</unit><unit label="article" level="2" identifier="C">ARTICLE C. - OFFICIAL TRAFFIC-CONTROL DEVICES AND SIGNS</unit></structure><referred_to_by/><text><section id="1108" prefix=""><text>It shall be unlawful to display any unauthorized traffic-control device which directs or purports to direct vehicular or pedestrian traffic.</text><type>section</type><prefixes><section/></prefixes><entire_prefix/><prefix_anchor/><level>1</level></section></text></law>
