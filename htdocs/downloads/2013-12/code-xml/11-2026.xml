<law><law_id>386</law_id><section_number>11-2026</section_number><catch_line>REMOVAL OF A TRAFFIC TICKET.</catch_line><history/><order_by>11-2026</order_by><metadata/><url>/11-2026/</url><token>11-2026</token><structure><unit label="division" level="1" identifier="II">Division II - Planning and Development</unit><unit label="part" level="3" identifier="11">PART 11 - TRANSPORTATION</unit><unit label="chapter" level="4" identifier="2">CHAPTER 2. - MOTOR VEHICLES AND TRAFFIC</unit><unit label="article" level="2" identifier="B">ARTICLE B. - ADMINISTRATION AND ENFORCEMENT</unit></structure><referred_to_by/><text><section id="1098" prefix=""><text>It shall be unlawful to remove a traffic ticket from a vehicle or to permit it to be removed, except for the purpose of answering the charge for which it was issued.</text><type>section</type><prefixes><section/></prefixes><entire_prefix/><prefix_anchor/><level>1</level></section></text></law>
